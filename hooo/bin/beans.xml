<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
      http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context-3.0.xsd">
      
       <bean id="adr1" class="com.springsdemo.Adress">
      <property name="city" value="banglore"/>
      <property name="state" value="ap"/>
      <property name="pincode" value="516464"/>
      
      </bean>
      
      <bean id="emp1" class="com.springsdemo.Employee12">
      
     
      <property name="emid" value="1"/>
      <property name="empname" value="krish"/>
      <property name="email" value="krishreddy460"/>
       <constructor-arg ref="adr1"/>
      
      </bean>
</beans>